<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dal.mapleLeafs.shelve12">


    <uses-permission android:name="android.permission.VIBRATE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="dal.mapleLeafs.shelve12.TermsActivity"></activity>
        <activity android:name="dal.mapleLeafs.shelve12.MedalsActivity" />
        <activity android:name="dal.mapleLeafs.shelve12.XIIStoreActivity" />
        <activity android:name="dal.mapleLeafs.shelve12.TutorialActivity" />
        <activity android:name="dal.mapleLeafs.shelve12.QRCodeActivity" />
        <activity
            android:name="dal.mapleLeafs.shelve12.SettingsActivity"
            android:label="@string/title_activity_settings" />
        <activity android:name="dal.mapleLeafs.shelve12.PlayActivity" />
        <activity
            android:name="dal.mapleLeafs.shelve12.DashboardActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name="dal.mapleLeafs.shelve12.MusicService"
            android:enabled="true" />
    </application>

</manifest>